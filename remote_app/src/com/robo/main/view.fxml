<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#pressedKeyAction" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.robo.main.View">
   <children>
      <Slider fx:id="sliderEngineLeft" layoutX="32.0" layoutY="130.0" min="-100.0" onMouseClicked="#engineLeftAction" onMouseDragged="#engineLeftAction" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" />
      <Slider fx:id="sliderEngineRight" layoutX="533.0" layoutY="130.0" min="-100.0" onMouseClicked="#engineRightAction" onMouseDragged="#engineRightAction" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" />
      <Button fx:id="connectButton" layoutX="328.0" layoutY="21.0" mnemonicParsing="false" onAction="#getIPAddressAction" text="Connect" />
      <TextField fx:id="IPAddress" layoutX="179.0" layoutY="21.0" promptText="IP address" />
   </children>
</AnchorPane>
